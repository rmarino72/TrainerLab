<configuration>
    <system.web>
        <compilation debug="true" targetFramework="4.6"/>
        <httpRuntime targetFramework="4.6"/>
        <customErrors mode="Off"/>
        <trust level="Full"/>
    </system.web>
    <system.webServer>
        <security>
            <authentication>
                <!--basicAuthentication enabled="false"/ -->
            </authentication>
            <requestFiltering>
                <requestLimits maxAllowedContentLength="52428800"/>
            </requestFiltering>
        </security>
        <modules>
            <remove name="WebDAVModule"/>
            <add name="BasicAuthHttpModule" type="TrainerLab.BasicAuthHttpModule, TrainerLab"/>
        </modules>
        <handlers>
            <remove name="WebDAV"/>
            <remove name="ExtensionlessUrl-Integrated-4.0"/>
            <add name="ExtensionlessUrl-Integrated-4.0" path="*." verb="GET,HEAD,POST,DEBUG,DELETE,PUT,PATCH"
                 type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0"/>
        </handlers>
    </system.webServer>
    <appSettings>
        <add key="Debug" value="true"/>
        <add key="DevDBHost" value="hostingmysql335.register.it"/>
        <add key="DevDBName" value="trainerlab_dev"/>
        <add key="DevDBPort" value="3306"/>
        <add key="DevDBUser" value="trainer_lab"/>
        <add key="DevDBPassword" value="Tr@1nerLab"/>
        <add key="ProdDBHost" value="hostingmysql335.register.it"/>
        <add key="ProdDBName" value="trainerlab"/>
        <add key="ProdDBPort" value="3306"/>
