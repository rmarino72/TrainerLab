        <add key="ProdDBPassword" value="Tr@1nerLab"/>
        <add key="LogName" value="TrainerLab"/>
        <add key="LogPath" value="Log/"/>
        <add key="Debug" value="true"/>
        <add key="Verbose" value="true"/>
        <add key="AppVersion" value="1.0"/>
        <add key="AppName" value="TrainerLab"/>
        <add key="DisplayName" value="Trainer Lab"/>
        <add key="Session" value="20"/>
        <add key="ImagePath" value="Images/"/>
        <add key="TemplatePath" value="Templates/"/>
        <add key="TmpPath" value="Tmp/"/>
    </appSettings>
